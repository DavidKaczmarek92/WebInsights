{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i styles/src/main.css -o app/styles/build/app.css -m",
    "dev": "concurrently \"npm run dev:css \" \"remix dev\"",
    "dev:css": "tailwindcss -m -i styles/src/main.css -o app/styles/build/app.css -w",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "storybook": "concurrently \"npm run storybook:css\" \"storybook dev -p 6006\"",
    "storybook:css": "tailwindcss -m -i styles/src/main.css -o .storybook/styles/build/app.css -w",
    "storybook:build": "storybook npm run storybook:build:css && build",
    "storybook:build:css": "tailwindcss -m -i styles/src/main.css -o .storybook/styles/build/app.css -m",
    "test": "jest",
    "test:watch": "jest --watch",
    "eslint": "npx eslint --max-warnings 0 --ext .ts,.tsx .",
    "stylelint": "stylelint \"**/*.css\""
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/serve": "^1.18.1",
    "@storybook/addon-a11y": "^7.0.25",
    "@types/node": "^20.10.0",
    "clsx": "^2.0.0",
    "isbot": "^3.6.8",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth-keycloak": "^1.2.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@storybook/addon-essentials": "^7.0.25",
    "@storybook/addon-interactions": "^7.0.25",
    "@storybook/addon-links": "^7.0.25",
    "@storybook/blocks": "^7.0.25",
    "@storybook/react": "^7.0.25",
    "@storybook/react-vite": "^7.0.25",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.6.1",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.6.1",
    "prettier": "3.0.0",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.25",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
